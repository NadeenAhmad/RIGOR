<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/cancer-ontology#"
     xml:base="http://example.org/cancer-ontology"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

    <owl:Ontology rdf:about="http://example.org/cancer-ontology"/>

    <owl:Class rdf:about="http://example.org/cancer-ontology#Cancer"/>
    <owl:Class rdf:about="http://example.org/cancer-ontology#Patient"/>
    <owl:Class rdf:about="http://example.org/cancer-ontology#Diagnosis"/>
    <owl:Class rdf:about="http://example.org/cancer-ontology#Treatment"/>

    <owl:ObjectProperty rdf:about="http://example.org/cancer-ontology#hasDiagnosis">
        <rdfs:domain rdf:resource="http://example.org/cancer-ontology#Patient"/>
        <rdfs:range rdf:resource="http://example.org/cancer-ontology#Diagnosis"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/cancer-ontology#hasTreatment">
        <rdfs:domain rdf:resource="http://example.org/cancer-ontology#Patient"/>
        <rdfs:range rdf:resource="http://example.org/cancer-ontology#Treatment"/>
    </owl:ObjectProperty>

</rdf:RDF>
